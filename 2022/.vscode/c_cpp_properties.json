{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**",
                // "C:/vcpkg/installed/x86-windows/include",
                // "C:/vcpkg/packages/gtest_x86-windows/include"
                "${workspaceFolder}/../vcpkg/installed/x86-windows/include",
                "${workspaceFolder}/../vcpkg/packages/gtest_x86-windows/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "windowsSdkVersion": "10.0.22000.0",
            "compilerPath": "cl.exe",
            "intelliSenseMode": "windows-msvc-x86"
            // "configurationProvider": "ms-vscode.cmake-tools"
        },
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/../vcpkg/installed/x64-linux/include",
                "${workspaceFolder}/../vcpkg/packages/gtest_x64-linux/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "compilerPath": "/bin/clang",
            "intelliSenseMode": "clang-x64"
            // "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
